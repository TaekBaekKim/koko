<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mbc.ktb555.login.LoginService">
<select id="idcount" resultType="int">
select count(*) from login0317 where id=#{param1}
</select>
 <insert id="loginsave">
insert into login0317 values(#{param1},#{param2},#{param3},#{param4})
 </insert>
 <select id="allout" resultType="com.mbc.ktb555.login.LoginDTO">
 select * from login0317
 </select>
</mapper>
